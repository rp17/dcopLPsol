set I;
var x00{i in I}, >= 0, <=1;
var x11{i in I}, >= 0, <=1;
minimize d: sum{i in I}x00[i];
s.t. total: sum{i in I}x11[i] = 1;
s.t. l0: x00[0] + x00[1] + x00[3] + x00[4] >= x11[4];
s.t. l1: x00[0] + x00[1] + x00[2] + x00[4] >= x11[1];
s.t. l2: x00[0] + x00[1] + x00[2] >= x11[2];
s.t. l3: x00[0] + x00[3] + x00[4] >= x11[3];
s.t. l4: x00[0] + x00[1] + x00[2] + x00[3] + x00[4] >= x11[0];
data;
set I := 0 1 2 3 4 ;
end;
